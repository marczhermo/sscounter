{
    "name": "marczhermo/sscounter",
    "description": "sscounter field",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "form",
        "admin"
    ],
    "require": {
        "php": ">=7.1",
        "silverstripe/framework": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "Marcz\\SSCounter\\": "code/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marcz\\SSCounter\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/ tests/behat/src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
