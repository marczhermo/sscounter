{
    "name": "marczhermo/sscounter",
    "description": "sscounter field",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "form",
        "admin",
    "react"
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "silverstripe/admin": "^2",
        "maxime-rainville/silverstripe-react": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "Marcz\\SSCounter\\": "code/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marcz\\SSCounter\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
